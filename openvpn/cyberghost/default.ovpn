dev tun 
client
persist-key
persist-tun
remote 87-1-nl.cg-dialup.net 443
nobind
proto udp
auth-user-pass


resolv-retry infinite 
redirect-gateway def1


cipher AES-256-CBC
ncp-disable
auth SHA256
ping 5
ping-exit 60
ping-timer-rem
explicit-exit-notify 2
script-security 2
remote-cert-tls server
route-delay 5
verb 4


ca ca.crt

cert client.crt

key client.key

