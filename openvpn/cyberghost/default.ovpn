dev tun
persist-key
persist-tun
nobind
remote 100-1-ro.cg-dialup.net 443
proto udp
auth-user-pass /config/openvpn-credentials.txt


resolv-retry infinite 
redirect-gateway def1
cipher AES-256-CBC
auth SHA256
ping 5
ping-exit 60
ping-timer-rem
explicit-exit-notify 2
script-security 2
remote-cert-tls server
route-delay 5
verb 4


#ca ca.crt

#cert client.crt

#key client.key

